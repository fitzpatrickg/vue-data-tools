{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:vue/essential",
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "no-console": 0,
        "eqeqeq": 0,
        "max-len": [
            1,
            120,
            4
        ],
        "no-param-reassign": 0,
        "consistent-return": 0,
        "radix": 0,
        "func-names": 0,
        "no-underscore-dangle": 0,
        "linebreak-style": 0
    }
}